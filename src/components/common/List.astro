---
const { text } = Astro.props;
---

<ul class="list-disc mx-2 px-4">
    <li class="my-3 text-lg"> { text } </li>
</ul>

<style>
    li {
      font-family:
        "Outfit Variable",
        system-ui,
        -apple-system,
        BlinkMacSystemFont,
        "Segoe UI",
        Roboto,
        Oxygen,
        Ubuntu,
        Cantarell,
        "Open Sans",
        "Helvetica Neue",
        sans-serif;
    }
  </style>